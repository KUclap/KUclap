{
  "private": true,
  "name": "KUclap",
  "version": "0.0.0",
  "license": "MIT",
  "homepage": "https://github.com/kuclap/KUclap",
  "scripts": {
    "dev": "NODE_ENV=development preact watch",
    "lint": "eslint src",
    "test": "jest",    
    "build:prod": "NODE_ENV=production preact build --prerenderUrls ./src/prerender-urls.js --template ./src/template.html --analyze ",
    "build:dev_prd": "NODE_ENV=development preact build --prerenderUrls ./src/prerender-urls.js --template ./src/template.html --analyze ",
    "build:dev": "NODE_ENV=development preact build --analyze ",
    "serve": "sirv build --cors --single",
    "start:dev": "NODE_ENV=development node server.js",
    "start:prod": "NODE_ENV=production node server.js",
    "webpack": "webpack --config webpack.config.js && babel-node ./dist/server.js",
    "predeploy:github-page": "npm run build:github-page",
    "build:github-page": "BASE_ROUTE=KUclap preact build --no-prerender && dot-json ./build/manifest.json start_url \"/KUclap/\"",
    "deploy:github-page": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-react-jsx": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "css-loader": "^3.6.0",
    "dot-json": "^1.2.0",
    "dotenv": "^8.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^2.0.0",
    "eslint": "^6.0.1",
    "eslint-config-preact": "^1.1.0",
    "file-loader": "^6.0.0",
    "gh-pages": "^2.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-preset-preact": "^1.0.0",
    "preact-async-route": "^2.2.1",
    "preact-cli": "^3.0.0-rc.6",
    "preact-cli-plugin-critical-css": "^0.1.0",
    "preact-render-spy": "^1.2.1",
    "sirv-cli": "^0.4.5",
    "svg-inline-loader": "^0.8.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.10",
    "@material-ui/icons": "^4.9.1",
    "animate.css": "^3.7.2",
    "axios": "^0.19.2",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "module-alias": "^2.2.2",
    "polka": "^0.5.2",
    "preact": "^10.5.3",
    "preact-helmet": "^4.0.0-alpha-3",
    "preact-lazy-route": "^1.0.1",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^3.2.1",
    "react-virtualized-select": "^3.1.3",
    "regenerator-runtime": "^0.13.5",
    "sirv": "^0.4.2",
    "styled-components": "^5.1.0",
    "styled-media-query": "^2.1.2"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  },
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat"
  },
  "_moduleAliases": {
    "react": "./node_modules/preact/compat/dist/compat.js",
    "react-dom": "./node_modules/preact/compat/dist/compat.js"
  }
}
