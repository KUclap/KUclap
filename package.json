{
  "private": true,
  "name": "KUclap",
  "version": "0.0.0",
  "license": "MIT",
  "homepage": "https://marsdev31.github.io/KUclap/",
  "scripts": {
    "build:preact": "GITHUB_PAGES=KUclap preact build --no-prerender && dot-json ./build/manifest.json start_url \"/KUclap/\"",
    "build": "preact build --no-prerender",
    "serve": "sirv build --cors --single",
    "dev": "preact watch",
    "lint": "eslint src",
    "test": "jest",
    "predeploy": "npm run build:gh",
    "deploy": "gh-pages -d build",
    "build:gh": "GITHUB_PAGES=KUclap preact build --no-prerender && dot-json ./build/manifest.json start_url \"/KUclap/\""
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "devDependencies": {
    "dot-json": "^1.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^2.0.0",
    "eslint": "^6.0.1",
    "eslint-config-preact": "^1.1.0",
    "gh-pages": "^2.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-preset-preact": "^1.0.0",
    "preact-async-route": "^2.2.1",
    "preact-cli": "^3.0.0-rc.6",
    "preact-render-spy": "^1.2.1",
    "sirv-cli": "^0.4.5"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.10",
    "@material-ui/icons": "^4.9.1",
    "animate.css": "^3.7.2",
    "axios": "^0.19.2",
    "preact": "^10.4.1",
    "preact-lazy-route": "^1.0.1",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^3.2.1",
    "react-virtualized-select": "^3.1.3",
    "styled-components": "^5.1.0",
    "styled-media-query": "^2.1.2",
    "webpack": "^4.43.0"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  }
}
